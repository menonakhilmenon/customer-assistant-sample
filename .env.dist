# Port to run the express server no need to change
EXPRESS_PORT=8087
# Enter OpenAI secret key below
SECRET_KEY=
# Enter port where redis would be running here, should be fine left alone if using the startup scripts
REDIS_URL=http://localhost:6379
# Path to the vector store, default should work fine, I've also added in some sample data at this path, so can be left untouched
VECTOR_STORE_PATH=./backend/vector-db
# Threshold of the confidence for filtering items from vector store for
# finding items associated with purpose, can be left alone
CONFIDENCE_THRESHOLD=1.3